generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model employees {
  id           Int        @unique @id @default(autoincrement()) @map("R_E_C_N_O_")
  badge        String     @map("CRACHA")
  employee     String     @map("FUNCIONARIO")

  @@map("funcionarios")
}


model historyAddress {
  id                    Int       @unique @id @default(autoincrement()) @map("R_E_C_N_O")
  odfNumber             String    @map("ODF")
  quantity              Int       @map("QUANTIDADE")
  status                String    @map("STATUS")

  @@map("historico_endereco")
}


model history_pointed {
  id                  Int     @unique @id @default(autoincrement()) @map("R_E_C_N_O_")
  odfNumber           Int     @map("ODF")
  pointedCode         Int     @map("CODAPONTA")
  statusPointedCode   String  @map("CAMPO2")

  @@map("hisaponta")
}