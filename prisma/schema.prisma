generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model funcionarios {
  R_E_C_N_O_        Int        @id @default(autoincrement()) 
  CRACHA            String     @unique
  FUNCIONARIO       String    
}


model historico_endereco {
  id      Int   @id @default(autoincrement())
  odfNumber             String 
}