generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model employees {
  id           Int        @id @default(autoincrement()) @map("R_E_C_N_O_")
  badge        String     @unique @map("CRACHA")
  employee     String     @map("FUNCIONARIO")

  @@map("funcionarios")
}


model historico_endereco {
  id      Int   @id @default(autoincrement())
  odfNumber             String 
}


model history_pointed {
  id            Int @unique @id @default(autoincrement()) @map("R_E_C_N_O_")
  odfNumber     Int @map("ODF")

  @@map("hisaponta")
}